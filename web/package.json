{
  "name": "ckb-explorer",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@cryptape/sdk-ts-config": "^0.0.1",
    "@types/jest": "24.0.11",
    "@types/node": "11.11.0",
    "@types/react": "16.8.7",
    "@types/react-dom": "16.8.2",
    "@types/react-router-dom": "^4.3.1",
    "@types/styled-components": "^4.1.12",
    "axios": "^0.18.1",
    "env-cmd": "^9.0.1",
    "history": "^4.7.2",
    "husky": "^1.3.1",
    "query-string": "6.4.2",
    "rc-pagination": "^1.17.11",
    "react": "^16.8.4",
    "react-app-rewired": "^2.1.0",
    "react-dom": "^16.8.4",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.8",
    "styled-components": "^4.1.3",
    "ts-jest": "^24.0.2",
    "typescript": "3.3.3333"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "1.4.0",
    "@typescript-eslint/parser": "1.4.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.0.1",
    "lint-staged": "^8.1.0",
    "prettier": "^1.16.0"
  },
  "scripts": {
    "start": "env-cmd -f ./.env.develop react-app-rewired start",
    "lint": "eslint src/**/*.{ts,tsx} --fix",
    "build": "env-cmd -f ./.env.production react-app-rewired build",
    "test": "jest",
    "eject": "react-app-rewired eject",
    "precommit": "lint-staged"
  },
  "jest": {
    "displayName": "ckb-explorer",
    "testRegex": "(/tests/.*.(test|spec))\\.(ts?|js?)$",
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint  src/**/*.{js,jsx,ts,tsx} --fix",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
