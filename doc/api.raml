#%RAML 1.0
title: CKB-Explorer
version: v1
baseUri: https://explorer.nervos.org
mediaType:  application/vnd.api+json
documentation:
 - title: Introduction
   content: |
     These are the APIs that serve the Explorer, we do not provide external services and cannot guarantee service availability.
     It is recommended to maintain the ckb node by yourself.
types:
  indicator: {
    type: string,
    enum: ["total_supply", "circulating_supply"]
  }
  error: {
    type: object,
    properties: {
      "id": {
        type: "string",
        description: "A unique identifier for the specific instance of the error"
      },
      "code": {
        type: "string",
        description: " an application-specific error code"
      },
      "status": {
        type: "string",
        description: "the HTTP status code applicable to this problem"
      },
      "title": {
        type: "string",
        description: "a short, human-readable summary of the problem"
      },
      "detail": {
        type: "string",
        description: "A more in-depth, human-readable description of the error and how to resolve it"
      },
      "href": {
        type: "string",
        description: "A URL that the developer can use to learn more about the issue"
      }
    }

  }

  transaction_attributes: {
    type: object,
    properties: {
      "block_number": {
        type: "string"
      },
      "transaction_hash": {
        type: "string"
      },
      "block_timestamp": {
        type: "string"
      }
      "transaction_fee": {
        type: "string"
      },
      "is_cellbase": {
        type: "boolean"
      },
      "income": {
        type: "string"
      },
      "version": {
        type: "string"
      },
      "display_inputs": {
        type: "display_input[]"
      },
      "display_outputs": {
        type: "display_output[]"
      },
      "witnesses": {
        type: "string[]"
      },
      "cell_deps": {
        type: "cell_dep[]"
      },
      "header_deps": {
        type: "string[]"
      }
    }
  }

  transaction: {
    type: object,
    properties: {
      "id": {
        type: "string"
      },
      "type": {
        type: "string"
      },
      "attributes": {
        type: "transaction_attributes",
      }
    }
  }

  address_attributes: {
    type: object,
    properties: {
      "address_hash": {
        "type": "string"
      },
      "balance": {
        "type": integer
      },
      "transactions_count": {
        "type": integer
      },
      "cell_consumed": {
        "type": integer
      },
      "lock_hash": {
        "type": script
      },
     "dao_deposit": {
        "type": string
      },
      "interest": {
        "type": string
      },
      "pending_reward_blocks_count": {
        "type": integer
      }
      "lock_info": {
        "type": lock_info_attributes
      }
    }
  }

  lock_info_attributes: {
    type: object,
    properties: {
      "status": {
        "type": string
        "enum": ["locked", "unlocked"]
      },
      "epoch_number": {
        "type": string
      },
      "epoch_index": {
       "type": string
      }
      "estimated_unlock_time": {
        "type": string
      }
    }
  }

  address: {
    type: object,
    properties: {
      "id": {
        type: "string"
      },
      "type": {
        type: "string"
      },
      "attributes": {
        type: "address_attributes"
      }
    }
  }

  script_attributes: {
    type: object,
    properties: {
      "code_hash": {
        "type": "string"
      },
      "args": {
        "type": "string[]"
      }
    }
  }

  script: {
    type: object,
    properties: {
      "id": {
        type: "string"
      },
      "type": {
        type: "string"
        "enum": ["lock_script", "type_script"]
      },
      "attributes": {
        type: "script_attributes"
      }
    }
  }

  display_input: {
    type: object,
    require: true,
    properties: {
      "from_cellbase": {
        type: "boolean"
      },
      "id": {
        type: "string"
      },
      "address_hash": {
        type: "string"
      },
      "capacity": {
        type: integer
      },
      "target_block_number": {
        type: integer,
        required: false
      },
      "generated_tx_hash": {
        type: "string",
      },
      "cell_type": {
        type: "string"
      },
      "started_block_number": {
        type: integer,
        required: false
      },
      "ended_block_number": {
        type: integer,
        required: false
      },
      "interest": {
        type: integer,
        required: false
      },
      "dao_type_hash": {
        type: "string"
        required: false
      }
    }
  }


  display_output: {
    type: object,
    require: true,
    properties: {
      "id": {
        type: "string"
      },
      "address_hash": {
        type: "string"
      },
      "capacity": {
        type: integer
      },
      "target_block_number": {
        type: integer,
        required: false
      },
      "base_reward": {
        type: integer,
        required: false
      },
      "commit_reward": {
        type: integer,
        required: false
      },
      "proposal_reward": {
        type: integer,
        required: false
      },
      "secondary_reward": {
        type: integer,
        required: false
      },
      "consumed_by_tx_hash": {
         type: "string",
         required: false
      },
      "status": {
        type: "string",
        enum: ["live", "dead"]
      }，
      "cell_type": {
        type: "string"
      },
      "dao_type_hash": {
        type: "string"
        required: false
      }
    }
  }

  block_attributes: {
    type: object,
    properties: {
      "block_hash": {
          "type": "string",
          "required": true,
          "minLength": 66,
          "maxLength": 66,
      },
      "number": {
        "type": "string"
      },
      "transactions_count": {
        "type": "string"
      },
      "proposals_count": {
        "type": "string"
      },
      "uncles_count": {
        "type": "string"
      },
      "uncle_block_hashes": {
        "type": "string[]"
      },
      "reward": {
        "type": "string"
      },
      "total_transaction_fee": {
        "type": "string"
      },
      "cell_consumed": {
        "type": "string"
      },
      "total_cell_capacity": {
        "type": "string"
      },
      "miner_hash": {
        "type": "string",
        "required": true,
        "minLength": 66,
        "maxLength": 66,
      },
      "timestamp": {
        "type": "string"
      },
      "difficulty": {
        "type": "string"
      },
      "version": {
        "type": "string"
      },
      "nonce": {
        "type": "string"
      },
      "proof": {
        "type": "string"
      },
      "reward_status": {
        "type": "string",
        "enum": ["pending", "issued"]
      },
      "received_tx_fee_status": {
        "type": "string",
        "enum": ["pending", "calculated"]
      },
      "received_tx_fee": {
        "type": "string"
      },
      "epoch": {
        "type": "string"
      },
      "length": {
        "type": "string"
      },
      "start_number": {
        "type": "string"
      },
      "block_index_in_epoch": {
        "type": "string"
      }
    }
  }

  out_point: {
    type: object,
    properties: {
      "index": {
        type: "integer"
      },
      "tx_hash": {
        type: "string"
      }
    }
  }

  cell_dep: {
    type: object,
    properties: {
      "dep_type": {
        type: "string",
        "enum": ["dep_group", "code"]
      },
      "out_point": {
        type: "out_point"
      }
    }
  }

  block: {
    type: object,
    properties: {
      "id": {
        type: "string"
      },
      "type": {
        type: "string"
      },
      "attributes": {
        "type": "block_attributes"
      }
    }
  }

  epoch_info_attributes: {
    type: object,
    properties: {
      "epoch_number": {
        type: "string"
      },
      "epoch_length": {
        type: "string"
      },
      "index": {
        type: "string"
      }
    }
  }

  index_statistic_attributes: {
    type: object,
    properties: {
      "tip_block_number": {
        type: "string"
      },
      "average_block_time": {
        type: "string"
      },
      "current_epoch_difficulty": {
        type: "string"
      },
      "hash_rate": {
        type: "string"
      },
      "epoch_info": {
        type: "epoch_info_attributes"
      },
      "estimated_epoch_time": {
        type: "string"
      },
      "transactions_last_24hrs": {
        type: "string"
      },
      "transactions_count_per_minute": {
        type: "string"
      }
    }
  }

  statistic_info_attributes: {
    type: object,
    properties: {
      "tip_block_number": {
        type: "string",
      },
      "average_block_time": {
        type: "string",
      },
      "current_epoch_difficulty": {
        type: "string",
      },
      "hash_rate": {
        type: "string",
      },
      "blockchain_info": {
        type: "object"
      },
      "address_balance_ranking": {
        type: "address_balance_ranking"
      }
    }
  }

  address_balance_ranking: {
    type: object,
    properties: {
      "ranking": {
        type: "string"
      },
      "address": {
        type: "string"
      },
      "balance": {
        type: "string"
      }
    }
  }

  net_info_address: {
    type: object,
    properties: {
      "address": {
        type: "string"
      },
      "score": {
        type: "string"
      }
    }
  }

  net_info_attributes: {
    type: object,
    properties: {
      "addresses": {
        type: "net_info_address[]",
      },
      "is_outbound": {
        type: "string",
      },
      "node_id": {
        type: "integer",
      },
      "version": {
        type: "string",
      }
    }
  }

  daily_statistic_attributes: {
    type: object,
    properties: {
      "transactions_count": {
        type: "string",
      },
      "addresses_count": {
        type: "string",
      },
      "total_dao_deposit": {
        type: "string",
      },
      "created_at_unixtimestamp": {
        type: "string",
      }
    }
  }

  block_statistic_attributes: {
      type: object,
      properties: {
        "difficulty": {
          type: "string",
        },
        "hash_rate": {
          type: "string",
        },
        "live_cell_count": {
          type: "string",
        },
        "dead_cell_count": {
          type: "string",
        },
        "block_number": {
          type: "string",
        }
      }
    }

  epoch_statistic_attributes: {
    type: object,
    properties: {
      "difficulty": {
        type: "string",
      },
      "uncle_rate": {
        type: "string",
      },
      "epoch_number": {
        type: "string",
      }
    }
  }

  dao_depositor_attributes: {
    type: "object",
    properties: {
      "address_hash": {
        type: "string"
      },
      "dao_deposit": {
        type: "string",
      }
    }
  }

  ckb_transaction_list_attributes: {
    type: "object",
    properties: {
      "transaction_hash": {
        type: "string"
      },
      "block_number": {
        type: "string"
      },
      "block_timestamp": {
        type: "string"
      },
      "capacity_involved": {
        type: "string"
      },
      "live_cell_changes": {
        type: "string"
      }
    }
  }

  index_statistic_info: {
    type: object,
    properties: {
      "id": {
        type: "string"
      },
      "type": {
        type: "string"
      },
      "attributes": {
        "type": "index_statistic_attributes"
      }
    }
  }

  pagination_meta: {
    type: object,
    properties: {
      "total": {
        type: "integer"
      }
    }
  }

  pagination_link: {
    type: object,
    properties: {
      "self": {
        type: "string"
      },
      "first": {
        type: "string"
      },
      "prev": {
        type: "string"
      },
      "next": {
        type: "string"
      },
      "last": {
        type: "string"
      },
    }
  }

  error_response: {
    type: object,
    properties: {
      "message": {
        "type: "string"
      },
      "errors": {
        "type": "error[]"
      }
    }
  }

  block_response: {
    type: object,
    properties: {
      "data": {
        "type": "block"
      }
    }
  }

  ckb_transaction_list_response: {
    type: object,
    properties: {
      "data": {
        "type": "ckb_transaction_list_attributes"
      }
    }
  }

  blocks_response: {
    type: object,
    properties: {
      "meta": {
        "type": "pagination_meta"
      },
      "data": {
        "type": "block[]",
      },
      "links": {
        "type": "pagination_link"
      }
    }
  }

  transaction_response: {
    type: object,
    properties: {
      "data": {
        "type": transaction,
      }
    }
  }

  transactions_response: {
    type: object,
    properties: {
      "meta": {
        "type": "pagination_meta"
      },
      "data": {
        "type": "transaction[]"
      },
      "links": {
        "type": "pagination_link"
      }
    }
  }

  block_transactions_response: {
    type: object,
    properties: {
      "meta": {
        "type": "pagination_meta"
      },
      "data": {
        "type": "transaction[]"
      },
      "links": {
        "type": "pagination_link"
      }
    }
