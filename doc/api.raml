      404:
        body:
          application/vnd.api+json:
            type: error_response
            example: |
              {
                "message": "Record Not Found",
                "errors": [
                  {
                    "id": "ActiveRecord::RecordNotFound",
                    "code": "1141",
                    "status": "404",
                    "title": "Record not fund",
                    "detail": "No blocks at this time",
                    "href": "https://nervosnetwork.github.io/ckb-explorer/public/api_doc.html"
